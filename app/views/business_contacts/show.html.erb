<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
  <h1 class="h2">Exibir</h1>
  <div class="btn-toolbar mb-2 mb-md-0">
    <div class="btn-group me-2">
      <%= link_to "Editar", edit_business_contact_path(@business_contact), class: 'btn btn-sm btn-outline-secondary' %>
      <%= link_to "Voltar para a listagem", business_contacts_path, class: 'btn btn-sm btn-outline-secondary' %>
    </div>
    <%= button_to "Excluir", @business_contact, method: :delete, class: 'btn btn-sm btn-outline-danger' %>
  </div>
</div>

<%= render @business_contact %>

<%= render 'nested_productive_properties' if @business_contact.kind == 'provider' %>

